//Escreva um algoritmo que leia uma medida em metros
// e converta para centímetros.

programa
{
	funcao inicio()
	{
		real x
		escreva("Informe a medida em metros: ")
		leia(x)

		x = x * 100

		x *= 100

		escreva("A medida em cm é: ",x," cm")
	}
}

